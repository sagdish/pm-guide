# For Vercel deployment, leave empty to use same domain
REACT_APP_BACKEND_URL=

# Backend Environment Variables (for API functions)
MONGO_URL=mongodb://your-mongo-connection-string
DB_NAME=your-database-name
SECRET_KEY=your-secret-key-change-in-production-pm-guide-2024